#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Tristan Tan/Documents/Degree CS/Final Year Subjects/Final Year Project/supergearyt/admin/node_modules/mime/node_modules:/mnt/c/Users/Tristan Tan/Documents/Degree CS/Final Year Subjects/Final Year Project/supergearyt/admin/node_modules:/mnt/c/Users/Tristan Tan/Documents/Degree CS/Final Year Subjects/Final Year Project/supergearyt/node_modules:/mnt/c/Users/Tristan Tan/Documents/Degree CS/Final Year Subjects/Final Year Project/node_modules:/mnt/c/Users/Tristan Tan/Documents/Degree CS/Final Year Subjects/node_modules:/mnt/c/Users/Tristan Tan/Documents/Degree CS/node_modules:/mnt/c/Users/Tristan Tan/Documents/node_modules:/mnt/c/Users/Tristan Tan/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Tristan Tan/Documents/Degree CS/Final Year Subjects/Final Year Project/supergearyt/admin/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Tristan Tan/Documents/Degree CS/Final Year Subjects/Final Year Project/supergearyt/admin/node_modules/mime/node_modules:/mnt/c/Users/Tristan Tan/Documents/Degree CS/Final Year Subjects/Final Year Project/supergearyt/admin/node_modules:/mnt/c/Users/Tristan Tan/Documents/Degree CS/Final Year Subjects/Final Year Project/supergearyt/node_modules:/mnt/c/Users/Tristan Tan/Documents/Degree CS/Final Year Subjects/Final Year Project/node_modules:/mnt/c/Users/Tristan Tan/Documents/Degree CS/Final Year Subjects/node_modules:/mnt/c/Users/Tristan Tan/Documents/Degree CS/node_modules:/mnt/c/Users/Tristan Tan/Documents/node_modules:/mnt/c/Users/Tristan Tan/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Tristan Tan/Documents/Degree CS/Final Year Subjects/Final Year Project/supergearyt/admin/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/cli.js" "$@"
else
  exec node  "$basedir/../mime/cli.js" "$@"
fi
